{
  "schema": "schema.graphql",
  "documents": "**/*.graphql",
  "extensions": {
    "strawberryShake": {
      "name": "CryptoClient",
      "namespace":"ChampWebApp.ClientGraphQL",
      "outputDirectoryName": "GeneratedComponents",
      "emitGeneratedCode": true,
      "url": "http://localhost:5009/graphql",
      "records": {
        "inputs": false,
        "entities": false
      },
      "transportProfiles": [
        {
          "default": "Http",
          "subscription": "WebSocket"
        }
      ]
    }
  }
}